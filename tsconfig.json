{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "lib": ["DOM", "ES6", "ES2016.Array.Include"],
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "ES5"
  },
  "include": [
    "src/**/*.ts"
  ]
}
